	Pages = {1985-92},
	Shorttitle = {Mitochondrial movement and positioning in axons: the role of growth factor signaling},
	Title = {Mitochondrial movement and positioning in axons: the role of growth factor signaling},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=12756280},
