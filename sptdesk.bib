	Number = {25},
	Pages = {11963-11968},
	Shorttitle = {E-cadherin tethered to micropatterned supported lipid bilayers as a model for cell adhesion},
	Title = {E-cadherin tethered to micropatterned supported lipid bilayers as a model for cell adhesion},
