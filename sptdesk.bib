	Number = {52},
	Pages = {19771-6},
	Shorttitle = {Anisotropy of cell adhesive microenvironment governs cell internal organization and orientation of polarity},
	Title = {Anisotropy of cell adhesive microenvironment governs cell internal organization and orientation of polarity},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=17179050},
