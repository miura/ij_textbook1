	Note = {0021-9533 (Print) Journal Article},
	Number = {Pt 16},
	Pages = {3635-44},
	Shorttitle = {Differential effects of a GTP-restricted mutant of Sar1p on segregation of cargo during export from the endoplasmic reticulum},
	Title = {Differential effects of a GTP-restricted mutant of Sar1p on segregation of cargo during export from the endoplasmic reticulum},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=15252131},
