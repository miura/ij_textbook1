	Number = {6},
	Pages = {2192-8},
	Shorttitle = {Local induction of acetylcholine receptor clustering in myotube cultures using microfluidic application of agrin},
	Title = {Local induction of acetylcholine receptor clustering in myotube cultures using microfluidic application of agrin},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=16387765},
	Volume = {90},
	Year = {2006},
