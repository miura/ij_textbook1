	Endnotereftype = {Journal Article},
	Journal = {J Cell Biol},
	Keywords = {Animals Cell Line Cell Nucleus/*ultrastructure Cytoplasm/ultrastructure Dextrans/diagnostic use Liver Neoplasms, Experimental/*ultrastructure Microscopy, Fluorescence Nuclear Envelope/ultrastructure Rats Research Support, Non-U.S. Gov't},
	Month = {Apr},
	Note = {0021-9525 Journal Article},
	Number = {4},
	Pages = {1183-90},
	Shorttitle = {Molecular mobility and nucleocytoplasmic flux in hepatoma cells},
	Title = {Molecular mobility and nucleocytoplasmic flux in hepatoma cells},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=2420804},
