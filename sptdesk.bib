	Month = {Jun},
	Note = {99284858 0006-3495 Journal Article},
	Number = {6},
	Pages = {3331-44},
	Shorttitle = {Anomalous diffusion of major histocompatibility complex class I molecules on HeLa cells determined by single particle tracking},
	Title = {Anomalous diffusion of major histocompatibility complex class I molecules on HeLa cells determined by single particle tracking},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=10354459},
