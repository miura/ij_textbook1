	Month = {Oct 20},
	Note = {97477418 0022-1007 Journal Article},
	Number = {8},
	Pages = {1347-55},
	Shorttitle = {Mutational evidence for control of cell adhesion through integrin diffusion/clustering, independent of ligand binding},
