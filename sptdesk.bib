	Keywords = {3T3 Cells Animals CHO Cells Cytoplasm/*physiology Diffusion Fluorescent Dyes Hamsters Humans Mice Microscopy, Fluorescence Plants Research Support, Non-U.S. Gov't Time Factors Viscosity},
	Month = {Apr 15},
	Note = {0003-9861 Journal Article},
	Number = {2},
	Pages = {159-67},
	Shorttitle = {Cell type and spatial location dependence of cytoplasmic viscosity measured by time-resolved fluorescence microscopy},
	Title = {Cell type and spatial location dependence of cytoplasmic viscosity measured by time-resolved fluorescence microscopy},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=9143317},
	Volume = {340},
