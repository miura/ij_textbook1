	Month = {May},
	Note = {21231377 1465-7392 Journal Article},
	Number = {5},
	Pages = {503-6},
	Shorttitle = {A 'hot-spot' mutation alters the mechanical properties of keratin filament networks},
	Title = {A 'hot-spot' mutation alters the mechanical properties of keratin filament networks},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=11331879},
