	Month = {Mar 4},
	Note = {0743-7463 (Print) Journal Article},
	Number = {5},
	Pages = {1611-7},
	Shorttitle = {Geometric determinants of directional cell motility revealed using microcontact printing},
	Title = {Geometric determinants of directional cell motility revealed using microcontact printing},
