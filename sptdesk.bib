	Number = {5},
	Pages = {369-78},
	Shorttitle = {Cell migration strategies in 3-D extracellular matrix: differences in morphology, cell matrix interactions, and integrin function},
	Title = {Cell migration strategies in 3-D extracellular matrix: differences in morphology, cell matrix interactions, and integrin function},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=9858334},
