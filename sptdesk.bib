	Title = {Measurement of Lagrangian velocity in fully developed turbulence},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=11736341},
