	Number = {21},
	Pages = {12021-6},
	Shorttitle = {High intranuclear mobility and dynamic clustering of the splicing factor U1 snRNP observed by single particle tracking},
	Title = {High intranuclear mobility and dynamic clustering of the splicing factor U1 snRNP observed by single particle tracking},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=11593012},
