	Endnotereftype = {Journal Article},
	Journal = {J Leukoc Biol},
	Keywords = {Actins/metabolism Animals Cell Adhesion *Cell Movement Cell Size *Chemotaxis, Leukocyte Cytoskeleton/physiology/ultrastructure Dictyostelium/cytology/growth & development/*physiology Extracellular Matrix/metabolism Leukocytes/*physiology *Models, Biological Receptors, Leukocyte-Adhesion/physiology Research Support, Non-U.S. Gov't Signal Transduction},
	Month = {Oct},
	Note = {0741-5400 (Print) Journal Article Review},
	Number = {4},
	Pages = {491-509},
	Shorttitle = {Amoeboid leukocyte crawling through extracellular matrix: lessons from the Dictyostelium paradigm of cell movement},
