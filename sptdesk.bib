	Month = {Jan},
	Note = {99093419 0006-3495 Journal Article},
	Number = {1 Pt 1},
	Pages = {573-9},
	Shorttitle = {Measurement of local viscoelasticity and forces in living cells by magnetic tweezers},
	Title = {Measurement of local viscoelasticity and forces in living cells by magnetic tweezers},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=9876170},
	Volume = {76},
