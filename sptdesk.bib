	Number = {5},
	Pages = {3336-52},
	Shorttitle = {Computational analysis of f-actin turnover in cortical actin meshworks using fluorescent speckle microscopy},
	Title = {Computational analysis of f-actin turnover in cortical actin meshworks using fluorescent speckle microscopy},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=12719263},
