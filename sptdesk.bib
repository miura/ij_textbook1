	Number = {10},
	Pages = {1035-47},
	Shorttitle = {Long-term stability of grafted polyethylene glycol surfaces for use with microstamped substrates in neuronal cell culture},
	Title = {Long-term stability of grafted polyethylene glycol surfaces for use with microstamped substrates in neuronal cell culture},
