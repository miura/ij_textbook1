	Keywords = {Alkaloids/pharmacology Azides/pharmacology Benzimidazoles/pharmacology Carcinoma, Squamous Cell Cell Line Cell Membrane/metabolism/ultrastructure Human Microscopy, Electron Microscopy, Phase-Contrast Microtubules/metabolism/ultrastructure Nocodazole Organoids/metabolism/ultrastructure Paclitaxel Receptors, Transferrin/drug effects/*metabolism Sodium Azide Support, Non-U.S. Gov't},
	Note = {88184685 0886-1544 Journal Article},
	Number = {1},
	Pages = {30-47},
	Shorttitle = {Dynamic behavior of the transferrin receptor followed in living epidermoid carcinoma (A431) cells with nanovid microscopy},
	Title = {Dynamic behavior of the transferrin receptor followed in living epidermoid carcinoma (A431) cells with nanovid microscopy},
