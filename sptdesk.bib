	Month = {Aug},
	Note = {20380705 0006-3495 Journal Article},
	Number = {2},
	Pages = {1095-106},
	Shorttitle = {Multiple-particle tracking measurements of heterogeneities in solutions of actin filaments and actin bundles},
	Title = {Multiple-particle tracking measurements of heterogeneities in solutions of actin filaments and actin bundles},
	Url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids=10920039},
